{
    "description": "Demonstrating webRequests",
    "manifest_version": 2,
    "name": "webRequest-demo",
    "version": "1.0",
    "permissions": [
        "webRequest",
        "<all_urls>",
        "webRequestBlocking",
        "storage",
        "tabs"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "web_accessible_resources": ["index.html" ]
}